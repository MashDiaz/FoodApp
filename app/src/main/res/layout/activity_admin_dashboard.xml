<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/passAdminLoginpass"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".AdminDashboard">

    <Button
        android:id="@+id/btnDiscountConfig"
        android:layout_width="166dp"
        android:layout_height="60dp"
        android:layout_marginEnd="32dp"
        android:drawableLeft="@drawable/baseline_add_alert_24"
        android:text="Discounts"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="260dp" />

    <Button
        android:id="@+id/btnadminAddItems"
        android:layout_width="156dp"
        android:layout_height="62dp"
        android:layout_marginStart="24dp"
        android:drawableLeft="@android:drawable/ic_menu_add"
        android:text="Add Items"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="#000000"
        tools:layout_editor_absoluteY="260dp" />

    <com.google.android.material.chip.ChipGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="290dp">

    </com.google.android.material.chip.ChipGroup>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout2"
        android:layout_width="409dp"
        android:layout_height="148dp"
        android:layout_marginTop="88dp"
        android:background="#FFEB3B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="81dp"
            android:layout_height="26dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:baselineAligned="false"
            android:fontFamily="sans-serif-black"
            android:text="Total Orders"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtTotalOrderCount"
            android:layout_width="79dp"
            android:layout_height="23dp"
            android:layout_marginStart="16dp"
            android:text="1000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.336" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="87dp"
            android:layout_height="22dp"
            android:layout_marginTop="16dp"
            android:fontFamily="sans-serif-black"
            android:text="Revenue "
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="116dp" />

        <TextView
            android:id="@+id/txtRevGenTotal"
            android:layout_width="87dp"
            android:layout_height="23dp"
            android:text="$100,000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.294"
            tools:layout_editor_absoluteX="116dp" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="76dp"
            android:fontFamily="sans-serif-medium"
            android:text="Pending Orders"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/txtPendingOrders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="36dp"
            android:text="100"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="396dp" />

</androidx.constraintlayout.widget.ConstraintLayout>